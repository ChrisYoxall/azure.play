{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft.Hosting": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} {SourceContext}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "CosmosDb": {
    "AccountEndpoint": "https://yoxall-demo-cosmos.documents.azure.com:443",
    "DatabaseName": "demo-db",
    "ContainerName": "items"
  },
  "EventGridTopic": {
    "endpoint": "https://demo-eventgrid-topic.australiaeast-1.eventgrid.azure.net/api/events"
  },
  "BlobStorage": {
    "ContainerName": "test-documents",
    "BlobServiceUrl": "https://yoxalldemostorage.blob.core.windows.net/"
  },
  "AllowedHosts": "*"
}
